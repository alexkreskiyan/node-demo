{
    "version": "0.2.0",
    "configurations": [{
        "type": "node",
        "request": "launch",
        "name": "launch server",
        "args": [
            "${workspaceFolder}/src/index.ts"
        ],
        "runtimeArgs": [
            "-r",
            "ts-node/register"
        ],
        "env": {
            "NODE_ENV": "development"
        },
        "cwd": "${workspaceFolder}",
        "protocol": "inspector",
        "internalConsoleOptions": "openOnSessionStart"
    }]
}